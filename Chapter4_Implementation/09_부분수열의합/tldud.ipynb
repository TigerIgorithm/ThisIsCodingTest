    "from itertools import combinations\n",
    "\n",
    "cnt = 0\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    combi = combinations(data, i)\n",
    "    for j in combi:\n",
    "        if sum(j) == s:\n",
    "            cnt +=1\n",
    "print(cnt)"
